<%= form_for(@inspection, url: product_inspection_path(@product, @inspection), html:{ class: 'form-horizontal'}) do |f| %>
    <%= render 'fields', f: f %>
    <% if logged_in? %>
      <%= f.submit '変更', class: 'col-xs-offset-2 btn btn-primary' %>
      <%= link_to '戻る', product_path(@product), class: 'btn btn-primary' %>
    <% else %>
      <%= link_to '戻る', product_path(@product), class: 'col-xs-offset-2 btn btn-primary' %>
    <% end %>
<% end %>